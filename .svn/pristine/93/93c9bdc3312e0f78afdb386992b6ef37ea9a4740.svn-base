<ion-header>

  <ion-navbar color="primary">
    <ion-title>Technical Documents</ion-title>
  </ion-navbar>

</ion-header>
<ion-content *ngIf="techDocument" padding>

  <ion-row>
    <ion-grid>
      <div class="name colm">
        Employee ID : {{techDocument.et_emp_id}}
      </div>
    </ion-grid>

    <ion-grid>
      <div class="name colm">
        Technical certificate 1
      </div>
      <div>
        <img src="data:image/png;base64,{{techDocument.et_tech_cert1}}"  alt="">
      </div>
      <ion-row>
        <ion-col>
          <button ion-button primary block>Validate</button>
        </ion-col>
        <ion-col>
          <button ion-button primary block>Resend</button>
        </ion-col>
      </ion-row>
    </ion-grid>

    <ion-grid>
      <div class="row2">
        Technical certificate 2 </div>
      <div>
        <img src="data:image/png;base64,{{techDocument.et_tech_cert2}}"  alt="">
      </div>
      <ion-row>
