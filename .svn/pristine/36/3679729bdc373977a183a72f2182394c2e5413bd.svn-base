<ion-header>

<ion-navbar color="primary">

    <ion-title>Documents</ion-title>
  </ion-navbar>

</ion-header>


<ion-content>
<div class="Adjust">
 
    <ion-grid>
      <ion-row>
        <ion-col>
          <ion-card (click)="viewEdu()">
            <img src="../assets/imgs/Graduate icon.png">
            <h6>Educational Certificates</h6>
          </ion-card>
        </ion-col>
        <ion-col>
          <ion-card (click)="viewTech()">
            <img src="../assets/imgs/importent.png">
            <h6>Technical Certificates</h6>
          </ion-card>
        </ion-col>
      </ion-row>
      <ion-row>
        <ion-col>
          <ion-card (click)="viewPer()">
            <img src="../assets/imgs/Personal icon.png">
            <h6>Personal Documents</h6>
          </ion-card>
        </ion-col>
        <ion-col>
          <ion-card (click)="viewProf()">
            <img src="../assets/imgs/experience.png">
            <h6>Professional Documents </h6>

          </ion-card>
        </ion-col>
      </ion-row>
    </ion-grid>
  </div>
</ion-content>
