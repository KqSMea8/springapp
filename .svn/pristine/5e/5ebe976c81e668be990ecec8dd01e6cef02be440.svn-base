<!--
  Generated template for the ViewDocumentPage page.

  See http://ionicframework.com/docs/components/#navigation for more info on
  Ionic pages and navigation.
-->
<ion-header>
  <ion-navbar color="primary">
    <ion-title *ngIf="!isOn" center>{{employeeTable?.ie_emp_name}}</ion-title>
      <ion-searchbar *ngIf="isOn"></ion-searchbar>
    <ion-buttons end>
      <button class="searchbutton" (click)="toggleDetails()"><ion-icon name="search" class="search"></ion-icon></button>
    </ion-buttons>
  </ion-navbar>
</ion-header>


<ion-content padding>
  <div class="item item-text-wrap" >

    <ion-grid>
      <ion-row>
        <ion-col>Employee ID </ion-col>
        <ion-col>:</ion-col>
        <ion-col>{{employeeTable?.ie_emp_id}}</ion-col>
      </ion-row>
    </ion-grid>
    <ion-grid>
      <ion-row>
        <ion-col>Email ID </ion-col>
        <ion-col>:</ion-col>
        <ion-col>{{employeeTable?.ie_emp_email}}</ion-col>
      </ion-row>
    </ion-grid>
    <ion-grid>
      <ion-row>
        <ion-col>1st Level Interviewer </ion-col>
        <ion-col>:</ion-col>
        <ion-col>{{employeeTable?.ie_int1}}</ion-col>
      </ion-row>
    </ion-grid>
     <ion-grid>
      <ion-row>
        <ion-col>2nd Level Interviewer </ion-col>
        <ion-col>:</ion-col>
        <ion-col>{{employeeTable?.ie_int2}}</ion-col>
      </ion-row>
    </ion-grid>
     <ion-grid>
      <ion-row>
        <ion-col>DOJ </ion-col>
        <ion-col>:</ion-col>
        <ion-col>{{employeeTable?.ie_emp_doj | date:'dd/MM/yyyy'}}</ion-col>
      </ion-row>
    </ion-grid>
     <ion-grid>
      <ion-row>
        <ion-col>Status </ion-col>
        <ion-col>:</ion-col>
        <ion-col>{{employeeTable?.ie_status}}</ion-col>
      </ion-row>
    </ion-grid>
 
  <div>
    <button ion-button primary block  (click)="viewDocs()">View Documents</button>
  </div>
  

  </div>
</ion-content>
