<ion-header>

  <ion-navbar color="primary">
    <ion-title>documents-upload</ion-title>
  </ion-navbar>

</ion-header>


<ion-content padding>
    <div class="progress-outer">
        <div class="progress-inner"  [style.width]="progress + '%'">
            {{progress}}%
        </div>
    </div>

  <ion-list no-lines>
    <ion-item *ngFor="let c of certificates">
      <ion-label>{{ c }}</ion-label>
      <button ion-button item-end color="danger">Upload</button>
    </ion-item>
  </ion-list>
  <button ion-button block item-end color="primary" (click)="gotodashboard()">Submit</button>
</ion-content>
