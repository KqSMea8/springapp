          <button ion-button primary block>Validate</button>
        </ion-col>
        <ion-col>
          <button ion-button primary block>Resend</button>
        </ion-col>
      </ion-row>
    </ion-grid>

    <ion-grid>

      <div class="row2">
        Technical certificate 3
      </div>
      <div>
        <img src="data:image/png;base64,{{profDocument.ep_service_cert3}}"  alt="">
      </div>
      <ion-row>
        <ion-col>
          <button ion-button primary block>Validate</button>
        </ion-col>
        <ion-col>
          <button ion-button primary block>Resend</button>
        </ion-col>
      </ion-row>
    </ion-grid>

    <ion-grid>

      <div class="row2">
        Payslip 1 </div>
      <div>
        <img src="data:image/png;base64,{{profDocument.ep_payslip1}}"  alt="">
      </div>
      <ion-row>
