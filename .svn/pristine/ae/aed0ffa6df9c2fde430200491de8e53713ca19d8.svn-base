<ion-header>

  <ion-navbar color="primary">
    <ion-title>Educational Certificates</ion-title>
  </ion-navbar>

</ion-header>


<ion-content padding>
  <ion-grid>
    <ion-row>
      <ion-col>
        <ion-card>
          <ion-card-content>
            <h6>SSLC</h6>
            <ion-item>
              <ion-label id="profile_image" color="primary" stacked></ion-label>
              <ion-input clearInput type="file" accept="image/*" (change)="changeListener($event)"></ion-input>
            </ion-item>
            <ion-buttons end>
              <button ion-button color="primary">Save</button>
            </ion-buttons>
          </ion-card-content>
      
         
        </ion-card>
      </ion-col>
    </ion-row>
